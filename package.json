  "devDependencies": {
    "@types/jest": "^23.3.13",
    "acorn": "^6.0.6",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.1",
    "terser-webpack-plugin": "^1.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "git add"
    ],
    "*.tsx": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jest.preprocessor.js"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ]
  }
}
